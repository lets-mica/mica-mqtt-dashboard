{
  "name": "mica-mqtt-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "Mica MQTT 服务端管理界面",
  "type": "module",
  "author": "如梦技术",
  "license": "MIT",
  "keywords": [
    "mqtt",
    "dashboard",
    "mica",
    "vue3",
    "typescript",
    "element-plus",
    "mqtt-client",
    "iot"
  ],
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "serve": "vite preview --port 4173",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss,md}\""
  },
  "dependencies": {
    "vue": "^3.5.28",
    "vue-router": "^4.6.4",
    "pinia": "^3.0.4",
    "axios": "^1.13.5",
    "element-plus": "^2.13.1",
    "@element-plus/icons-vue": "^2.3.1",
    "mqtt": "^5.15.0",
    "dayjs": "^1.11.19",
    "echarts": "^5.4.3",
    "vue-echarts": "^6.6.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.8.0",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "vite": "^7.3.1",
    "vue-tsc": "^3.2.4"
  }
}
